{
  "name": "john-pratt.com",
  "description": "Portfolio Site v3",
  "version": "1.1.0",
  "author": "John Pratt <john@john-pratt.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpratt9/john-pratt.com"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=24.9.0",
    "npm": ">=11.6.0"
  },
  "scripts": {
    "prebuild": "npm cache clean --force; npm install --force",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky",
    "test": "echo \"No tests yet\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },

  "dependencies": {
    "animejs": "^4.2.0",
    "babel-plugin-styled-components": "^1.12.0",
    "bluebird": "^3.7.2",
    "gatsby": "^5.15.0",
    "gatsby-plugin-google-analytics": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-styled-components": "^6.15.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.15.0",
    "gatsby-remark-prismjs": "^7.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.20.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-transition-group": "^4.4.5",
    "scrollreveal": "^4.0.9",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "babel-preset-gatsby": "^3.15.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "gatsby-remark-code-titles": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "schema-utils@^4": {
      "ajv": "8.17.1",
      "ajv-formats": "2.1.1",
      "ajv-keywords": "5.1.0"
    },
    "ajv-keywords@^5": {
      "ajv": "8.17.1"
    },
    "schema-utils@^3": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    },
    "schema-utils@^2": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    }
  }
}
