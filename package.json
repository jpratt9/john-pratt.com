{
  "name": "john-pratt.com",
  "description": "Portfolio Site v3",
  "version": "1.1.0",
  "author": "John Pratt <john@john-pratt.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpratt9/john-pratt.com"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=24.9.0",
    "npm": ">=11.6.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "cache-nuke": "npm cache clear --force; rm -rf node_modules .cache package-lock.json public; npm i --force --legacy-peer-deps",
    "build": "npm run type-check && npm run test:run && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "start": "gatsby serve",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint-staged": "lint-staged",
    "verify-no-js": "bash -c 'JS_COUNT=$(find src -name \"*.js\" -type f | wc -l); if [ $JS_COUNT -eq 0 ]; then echo \"No JavaScript files in src/\"; else echo \"Found $JS_COUNT JavaScript files in src/:\"; find src -name \"*.js\" -type f; exit 1; fi'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "animejs": "4.2.0",
    "babel-plugin-styled-components": "1.12.0",
    "bluebird": "3.7.2",
    "gatsby": "5.15.0",
    "gatsby-adapter-netlify": "1.2.1",
    "gatsby-plugin-google-gtag": "5.15.0",
    "gatsby-plugin-image": "3.15.0",
    "gatsby-plugin-manifest": "5.15.0",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-page-creator": "5.16.0-next.0",
    "gatsby-plugin-preact": "7.15.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.15.0",
    "gatsby-plugin-sitemap": "6.15.0",
    "gatsby-plugin-styled-components": "6.15.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "7.15.0",
    "gatsby-remark-prismjs": "7.15.0",
    "gatsby-source-filesystem": "5.15.0",
    "gatsby-transformer-remark": "6.15.0",
    "gatsby-transformer-sharp": "5.15.0",
    "lodash": "4.17.21",
    "prismjs": "1.20.0",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-transition-group": "4.4.5",
    "scrollreveal": "4.0.9",
    "sharp": "^0.34.5",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/eslint-parser": "7.28.4",
    "@babel/helper-plugin-utils": "7.27.1",
    "@babel/preset-react": "7.27.1",
    "@gatsbyjs/webpack-hot-middleware": "^2.25.3",
    "@prefresh/babel-plugin": "^0.5.2",
    "@prefresh/core": "^1.5.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-transition-group": "^4.4.12",
    "@types/scrollreveal": "^0.0.11",
    "@types/styled-components": "^5.1.35",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "ajv": "^7.2.4",
    "babel-preset-gatsby": "3.15.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "gatsby-plugin-perf-budgets": "0.0.18",
    "gatsby-plugin-typescript": "^5.15.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.32",
    "gatsby-remark-code-titles": "1.1.0",
    "husky": "9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "16.2.3",
    "preact": "^10.27.2",
    "preact-render-to-string": "^6.6.2",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "webpack": "5.98.0"
  },
  "overrides": {
    "webpack": "5.98.0",
    "webpack-dev-middleware": "5.3.4",
    "@prefresh/webpack": "^4.0.5",
    "schema-utils@^4": {
      "ajv": "8.17.1",
      "ajv-formats": "2.1.1",
      "ajv-keywords": "5.1.0"
    },
    "ajv-keywords@^5": {
      "ajv": "8.17.1"
    },
    "schema-utils@^3": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    },
    "schema-utils@^2": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    }
  }
}
